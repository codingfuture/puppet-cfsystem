{
  "name": "codingfuture-cfsystem",
  "version": "0.12.5",
  "author": "Andrey Galkin",
  "summary": "Configure a bare minimal production system regardless of its purpose",
  "license": "Apache-2.0",
  "source": "https://github.com/codingfuture/puppet-cfsystem",
  "project_page": "https://github.com/codingfuture/puppet-cfsystem",
  "issues_url": "https://github.com/codingfuture/puppet-cfsystem/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "16.04",
        "16.10",
        "17.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.7.0 <6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.24.0 <5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 4.5.1 <5.0.0"
    },
    {
      "name": "puppetlabs-git",
      "version_requirement": ">= 0.5.0 <1.0.0"
    },
    {
      "name": "herculesteam/augeasproviders_sysctl",
      "version_requirement": ">= 2.2.0 <3.0.0"
    },
    {
      "name": "codingfuture-cfnetwork",
      "version_requirement": ">= 0.12.1 < 0.13.0"
    },
    {
      "name": "codingfuture-cfauth",
      "version_requirement": ">= 0.12.0 < 0.13.0"
    }
  ],
  "data_provider": "hiera"
}
